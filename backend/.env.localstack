API_V1_STR=/api/v1
DEBUG=true

# PostgreSQL Database
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=backend
POSTGRES_PORT=5432

# AI Configuration  
AI_DEFAULT_PROVIDER=openai
OPENAI_API_KEY=test-key-for-dev
